  BITree(int);
  void ConstructBITree();
  int GetSum();



};
BITree::BITree(int n){
  no_of_nodes = n;
  bindex_tree = new int[no_of_nodes+1];
  for(int i=1;i<=no_of_nodes;++i){
    bindex_tree[i] = 0;
  }
}

